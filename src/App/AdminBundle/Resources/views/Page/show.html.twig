{% extends isAjax ? 'AppAdminBundle:Shared:ajax.html.twig' : 'AppAdminBundle:Shared:base.html.twig' %}

{% block content %}
	{% include 'AppAdminBundle:Page:pane/show.html.twig' %}
{% endblock %}

{% block javascripts %}
	<script>
		$(function() {
			var frame = document.getElementById('adm_page_frame');
			
			$('#adm_page_frame_reload').click(function() {
				frame.contentWindow.location.reload(true);
			});
		});
	</script>
{% endblock %}